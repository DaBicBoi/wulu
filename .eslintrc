{
  "parser": "babel-eslint",
  "globals": {
    "Bot": false,
    "Cidr": false,
    "CommandParser": false,
    "Config": false,
    "Dnsbl": false,
    "LoginServer": false,
    "toId": false,
    "toName": false,
    "ResourceMonitor": false,
    "Rooms": false,
    "Simulator": false,
    "Sockets": false,
    "string": false,
    "TeamValidator": false,
    "Tools": false,
    "Tournaments": false,
    "Users": false,
    "Verifier": false,
    "Wulu": false
